          dots: true, //スライドの下にドットのナビゲーションを表示
          autoplay: true, //自動再生
          autoplaySpeed: 4000, //再生スピード
          });
        var subscriptions_count = $('#subscriptions').data("subscriptions-count");
        var slidesToshow_val = 3;  //ここからオススメ画像スライダー   スライダーに表示させるデフォルト画像表示枚数
        var slidesToshow_val_breakpoint_1 = 2; // breakpoint1 960px時のデフォルト画像表示枚数

        var slidesToshow_leng =  subscriptions_count;
                // @subscriptions（おすすめ店舗）が0,1,2枚 < 3枚 だったら
        if( subscriptions_count < slidesToshow_val){
          slidesToshow_val = 1;
          slidesToshow_val_breakpoint_1 = 1;
        }

        $('.autoplay').slick({
          slidesToShow: slidesToshow_val,
          slidesToScroll: 1,
          centerMode: true, //要素を中央寄せにする
          autoplay: true,
          infinite: true,
          autoplaySpeed: 2000,
          responsive: [{
                        breakpoint: 960,  //画面幅960pxで以下のセッティング
                              settings: {
                                  slidesToShow: slidesToshow_val_breakpoint_1,
                                  slidesToScroll: 1,
                        }
                    },{
                        breakpoint: 720,   //画面幅720pxで以下のセッティング
                              settings: {
                                  slidesToShow: 1,
                                  slidesToScroll: 1,
                              }
                        }
                    ]
        });
      });                     //ここまでオススメ画像スライダー
    </script>

